<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MusicDefault" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/card_alt</item>
        <item name="android:navigationBarColor">?attr/card_alt</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>

        <item name="bg">@color/material_dynamic_neutral10</item>
        <item name="fg">@color/material_dynamic_neutral80</item>
        <item name="fg_disabled">@color/material_dynamic_neutral60</item>
        <item name="fg_accent">@color/material_dynamic_primary70</item>
        <item name="fg_accent2">@color/material_dynamic_primary60</item>
        <item name="borders">@color/material_dynamic_neutral20</item>
        <item name="card">@color/material_dynamic_neutral_variant30</item>
        <item name="card_alt">@color/material_dynamic_neutral_variant20</item>
        <item name="cardborder">@color/dark_card</item>
        <item name="fg_alpha">@color/white_alpha</item>
        <!-- Customize your theme here. -->

        <item name="android:windowBackground">?attr/bg</item>
        <item name="android:textColorPrimary">?attr/fg</item>
    </style>
</resources>